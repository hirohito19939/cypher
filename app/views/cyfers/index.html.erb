<center><h2>サイファー一覧</h2></center>
<hr>
<div class="clearfix">
<ul>
      <% @cyfers.each do |cyfer| %>
        <li class="cyferbox">
          <center>
            <%= image_tag cyfer.avatar, :size => "100pxx300px" %>
            </center>
            <p><a href="/cyfers/<%= cyfer.id %>">サイファーの詳細</a></p>
            <h3 class="cyfer-name"><%= cyfer.name %></h3>
            <br>
        </li>
      <% end %>
    </div>
</ul>